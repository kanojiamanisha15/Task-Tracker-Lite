{
  "name": "task-tracker-backend",
  "version": "1.0.0",
  "description": "Backend API for Task Tracker Lite application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:sync": "node scripts/db-manager.js sync",
    "db:seed": "node scripts/db-manager.js seed",
    "db:reset": "node scripts/db-manager.js reset",
    "db:reset-seed": "node scripts/db-manager.js reset-seed",
    "db:status": "node scripts/db-manager.js status",
    "db:setup": "npm run db:sync && npm run db:seed",
    "db:test": "node scripts/test-db.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["task", "tracker", "api", "express", "mysql"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
